# $XConsortium: Library.tmpl,v 1.12 92/03/20 15:05:19 rws Exp $

_NULLCMD_ = @ echo -n

         CC = $(TOOLS)/acc -D__alpha -I -L -L$(CROSSBASE)/shlib -L$(CROSSBASE)/ccs/lib $(CALLSHAREDSWITCH)
  CCOPTIONS = 	-Wf,-XNd10000 -Wf,-XNp20000
STD_DEFINES = 	-DDEC -DR5_XLIB -DDECHINTS -DSMT
CDEBUGFLAGS = -O

   INSTALLFLAGS = $(INSTINCFLAGS)
       LINTLIBS = $(LINTXLIB)

HEADERS = Stuff.h
   SRCS = Stuff.c
   OBJS = Stuff.o

   LIBRARY_NAME = Stuff

# # Wcl Library Template for X11R5
# #

      LIBRARY_g = $(LIBRARY_NAME)_g
      LIBRARY_p = $(LIBRARY_NAME)_p

all::
	 $(_NULLCMD_)
	 $(_NULLCMD_)
	 $(_NULLCMD_)

includes::
	 $(_NULLCMD_)
	 $(_NULLCMD_)
	 $(_NULLCMD_)

.c.o:
	 $(_NULLCMD_)
	 $(_NULLCMD_)
	 $(_NULLCMD_)
	    $(RM) $@
	$(CC) -c $(CFLAGS) $(_NOOP_) $(SHLIBDEF) $(SHAREDCODEDEF) $(PICFLAGS) $*.c

clean::
	 $(_NULLCMD_)
	 $(_NULLCMD_)
	 $(_NULLCMD_)

all:: lib$(LIBRARY_NAME).a

lib$(LIBRARY_NAME).a:  $(OBJS) $(UNSHAREDOBJS)
	$(RM) $@
	$(AR) $@ $(OBJS) $(UNSHAREDOBJS)

all:: lib$(LIBRARY_NAME).so

lib$(LIBRARY_NAME).so:  lib$(LIBRARY_NAME).a
	$(RM) $@
	$(LD) $(SHAREDREGISTRY) -shared -all \
		-o $@ lib$(LIBRARY_NAME).a \
		-none -L$(USRSHLIBDIR) $(SO_REQLIBS)

clean::
	$(RM) lib$(LIBRARY_NAME).a  lib$(LIBRARY_NAME).so

lintlib:: llib-l$(LIBRARY_NAME).ln

llib-l$(LIBRARY_NAME).ln: $(SRCS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)$(LIBRARY_NAME) $(LINTFLAGS) $(SRCS)

includes::
	 @if [ -d $(BUILDINCDIR)/$(LIBRARY_NAME) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(BUILDINCDIR)/$(LIBRARY_NAME)); fi
	@(set -x; cd $(BUILDINCDIR)/$(LIBRARY_NAME); for i in $(HEADERS); do \
		$(RM) $$i; \
		$(LN) $(BUILDINCTOP)/../$(CURRENT_DIR)/$$i .; \
	done)

depend:: $(DEPEND)

$(DEPEND):
	@echo "checking $@ over in $(DEPENDSRC) first..."; \
	cd $(DEPENDSRC); $(MAKE); \
	echo "okay, continuing in $(CURRENT_DIR)"

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

