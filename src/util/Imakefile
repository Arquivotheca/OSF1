#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)' 'BOOTSTRAPCFLAGS=$(BOOTSTRAPCFLAGS)'

#if !UseCCMakeDepend
  MDEP_DIR = makedepend_local makedepend
#endif

SUBDIRS = scripts $(MDEP_DIR)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))

Makefiles:: Locals

Locals:
	( \
	cd makedepend_local; \
	rm -rf *.[ch]; \
	for i in ../makedepend/\*.[ch]  ; \
	do \
		ln -sf $$i . ; \
	done ; \
	); 
