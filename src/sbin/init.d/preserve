set `who -r`
if [ "$9" != "S" ]
then
        exit
fi

#
# Preserve any editor files
#

echo "Preserving editor files"
(cd /tmp && /usr/lbin/expreserve -a)
(cd /var/tmp && /usr/lbin/expreserve -a)



