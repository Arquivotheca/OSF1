SRCS=	\
	periodic.c \

all:: periodic

periodic:: periodic.o periodic.uid $(DEPS)
	$(RM) $@
	$(CC) -o $@ periodic.o $(DEPS) $(LOCAL_LIBRARIES) $(LDFLAGS) $(SYSLIBS)

periodic.uid:: periodic.uil
	$(RM) $@
	$(UIL) -o $@ periodic.uil $(UILFLAGS) $(UILINCLUDES)

clean::
	$(RM) periodic
	$(RM) periodic.uid
	$(RM) periodic.o

install:: periodic
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  periodic $(DESTDIR)$(BINDIR)

install:: periodic.uid
	$(INSTALL) -c $(INSTDATFLAGS) periodic.uid $(DESTDIR)$(LIBDIR)/uid

install:: Xcd.ad
	@if [ -d $(DESTDIR)$(XAPPLOADDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(XAPPLOADDIR)); fi
	$(INSTALL) -c $(INSTAPPFLAGS) Xcd.ad $(DESTDIR)$(XAPPLOADDIR)/Xcd

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

