!+
!
! Customize general dialog box
!
!-

! The FormDialog-ScrolledWindow-Form structure is so that we can muck with
!	the sizes at run-time so that we can get automatic scrollbars
!	for small screen stuff.

object customize_general_box : XmFormDialog
{
    arguments
    {
	XmNmarginHeight = k_top_margin;
	XmNmarginWidth = k_left_margin;
	XmNhorizontalSpacing = k_left_margin;
	XmNverticalSpacing = k_top_margin;
	XmNautoUnmanage = false;
	XmNallowOverlap = false;
	XmNdialogStyle = XmDIALOG_MODELESS;
	XmNdialogTitle = k_customize_general_text;
	XmNnoResize = true;
	XmNdefaultButton = XmPushButton general_ok_button;
	XmNcancelButton = XmPushButton general_cancel_button;
	XmNshadowType = XmSHADOW_OUT;
	XmNresizePolicy = XmRESIZE_GROW;
    };
    controls
    {
	XmForm		general_misc0_form;
	XmForm		general_misc_form;
	XmForm		general_buttons_form;
	XmLabel		general_dummy_label;
    };
    callbacks
    {
	XmNhelpCallback = procedure dwc_help_for_widget_activate
	   (hlp_ovr_optmen_gen);
    };
};

