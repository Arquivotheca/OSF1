    arguments
    {
	XmNvalue = 1;
	XmNminimum = 1;
	XmNmaximum = 31;
!	XmNprocessingDirection = XmMAX_ON_RIGHT;
	XmNorientation = XmHORIZONTAL;
	XmNshowValue = true;
	XmNhighlightThickness = 1;
	XmNtitleString = k_week1_text;
!HACK - XmNscaleWidth doesn't work, although XmNscaleHeight does.
	XmNwidth = 100;
	XmNtopAttachment = XmATTACH_FORM;
	XmNleftAttachment = XmATTACH_FORM;
	XmNrightAttachment = XmATTACH_FORM;
    };
    callbacks
    {
	XmNhelpCallback = procedure dwc_help_for_widget_activate
	    (hlp_ovr_optmen_gen_1stwkofyear);
	MrmNcreateCallback = procedure generic_create_proc( k_ysday );
	XmNvalueChangedCallback = procedure first_week_consistency();
    };
};

