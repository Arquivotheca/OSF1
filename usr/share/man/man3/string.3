\*Lmemccpy\*O(3),
\*Lsetlocale\*O(3),
\*Lstrchr\*O(3),
\*Lstrcoll\*O(3),
\*Lstrlen\*O(3),
\*Lstrncat\*O(3)/\*Lstrncmp\*O(3)/\*Lstrncpy\*O(3),
\*Lstrpbrk\*O(3),
\*Lstrspn\*O(3),
\*Lstrtok\*O(3),
\*Lstrstr\*O(3),
\*Lstrxfrm\*O(3),
\*Lswab\*O(3).
\*Lwcscat\*O(3)/\*Lwcscmp\*O(3)/\*Lwcscpy\*O(3).
