\*Lres_send\*O(3),
\*Ldn_comp\*O(3),
\*Ldn_expand\*O(3),
\*Lgetshort\*O(3),
\*Lgetlong\*O(3),
\*Lputshort\*O(3),
\*Lputlong\*O(3),
\*Ldn_skipname\*O(3)
.PP 
Commands: \*Lnamed\*O(8)
.iX "dn_find function"
.EQ
