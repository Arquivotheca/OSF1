\*Lres_mkquery\*O(3),
\*Lres_send\*O(3),
\*Ldn_comp\*O(3),
\*Ldn_find\*O(3),
\*Lgetshort\*O(3),
\*Lgetlong\*O(3),
\*Lputshort\*O(3),
\*Lputlong\*O(3),
\*Ldn_skipname\*O(3)
.iX "dn_expand function"
.EQ
delim off
.EN
